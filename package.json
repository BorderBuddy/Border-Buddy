{
  "name": "BorderBuddy",
  "version": "0.0.1",
  "description": "Your Border Buddy",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets es2015,stage-2",
    "dev": "webpack-dev-server --config webpack.dev.babel.js",
    "build": "webpack --config webpack.prod.babel.js",
    "test-redux": "jest",
    "test": "NODE_ENV=test ./run-tests.sh && npm run test-redux",
    "test-local": "NODE_ENV=test LOCAL=1 ./run-tests.sh && npm run test-redux",
    "seed": "babel-node database/seed.js",
    "deploy-seed": "babel-node database/deploymentSeed.js",
    "worker": "babel-node cron",
    "migrate": "sequelize db:migrate",
    "test:migrate": "NODE_ENV=test sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "push:acceptance": "cf push -f manifest.acceptance.yml",
    "push:production": "cf push -f manifest.production.yml"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EmilyDev/Hack-The-Ban.git"
  },
  "keywords": ["hacktheban"],
  "author": "emilyho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EmilyDev/Hack-The-Ban.git"
  },
  "jest": {
    "globals": {
      "window": true
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost"
  },
  "homepage": "https://github.com/EmilyDev/Hack-The-Ban.git",
